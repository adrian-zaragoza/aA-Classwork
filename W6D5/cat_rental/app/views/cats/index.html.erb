<h1>Show all the cats</h1>

<ul>
<% @cats.each do |cat| %>
  <li>
    <%=cat.name%>
  </li>
  <li>
    <%=cat.birth_date%>
  </li>
  <li>
    <%=cat.age%>
  </li>
    <li>
    <%=cat.color%>
  </li>
  <li>
    <%=cat.sex%>
  </li>
  <li>
    <%=cat.description%>
  </li>
  <li>
    <a href = <%= cat_url(cat) %>>Cat Page</a>
  </li>
  <% end %>
</ul>


